{
    "compilerOptions": {
      "target": "ESNext",
      "module": "CommonJS",
      "lib": ["ESNext", "DOM"],
      "moduleResolution": "Node",
      "resolveJsonModule": true,
      "esModuleInterop": true,
      "isolatedModules": true,
      "allowSyntheticDefaultImports": true,
      "strict": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "noEmit": true
    },
    "include": ["./tests/**/*.ts", "./pages/**/*.ts", "./utils/**/*.ts"]
  }
  